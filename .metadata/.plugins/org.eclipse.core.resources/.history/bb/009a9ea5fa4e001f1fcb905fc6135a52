package com.techlabs.creational.singleton.model;

public class Login {
	
	private Login login;
	
	private Login()
	{
		System.out.println("Login Created");
	}
	
	public Login getLogin()
	{
		login=new Login();
		return login;
	}
	
	public void status()
	{
		System.out.println("Login Successful");
	}

}
